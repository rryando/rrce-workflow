{
  "name": "rrce-workflow",
  "version": "0.2.62",
  "description": "RRCE-Workflow TUI - Agentic code workflow generator for AI-assisted development",
  "author": "RRCE Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rryando/rrce-workflow.git"
  },
  "homepage": "https://github.com/rryando/rrce-workflow#readme",
  "bugs": {
    "url": "https://github.com/rryando/rrce-workflow/issues"
  },
  "keywords": [
    "ai",
    "workflow",
    "copilot",
    "agent",
    "tui",
    "cli",
    "code-generation",
    "agentic"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "rrce-workflow": "bin/rrce-workflow.js"
  },
  "files": [
    "dist",
    "agent-core",
    "docs",
    "bin"
  ],
  "scripts": {
    "dev": "npx tsx src/index.ts",
    "wizard": "npx tsx src/index.ts wizard",
    "select": "npx tsx src/index.ts select",
    "start": "npx tsx src/index.ts",
    "build": "esbuild src/index.ts --bundle --platform=node --format=esm --outfile=dist/index.js --packages=external",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@clack/core": "^0.5.0",
    "@clack/prompts": "^0.11.0",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@xenova/transformers": "^2.17.2",
    "gray-matter": "^4.0.3",
    "ink": "^6.6.0",
    "ink-link": "^5.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "picocolors": "^1.1.1",
    "react": "^19.2.3",
    "yaml": "^2.8.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "esbuild": "^0.27.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
